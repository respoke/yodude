{
    "rules": {
        "indent": [
            2,
            4
        ],
        "camelcase": 0,
        "quotes": [0],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-use-before-define": [
            2,
            "nofunc"
        ],
        "guard-for-in": 0,
        "wrap-iife": [2, "inside"],
        "global-strict": 2,
        "no-underscore-dangle": 0,
        "consistent-return": 0,
        "new-cap": 0,
        "handle-callback-err": 0,
        "no-shadow": 0,
        "no-use-before-define": 0,
        "no-empty": 0,
        "no-process-exit": 0,
        "no-alert": 0,
        "valid-typeof": 0
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "mocha": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "plugins": [],
    "globals": {
        "respoke": true
    }
}
